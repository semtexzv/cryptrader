library(ggrepel)
files <- list.files(pattern = "*.csv")
p <- ggplot()
for (i in seq_along(files)) {
file <- files[i]
data = read.csv(file,header = FALSE)
data$save <- as.numeric(data$V1)
data$dispatch <- as.numeric(data$V2)
data$load <- as.numeric(data$V3)
data$exec <- as.numeric(data$V4)
data_summary <- function(data, varname, groupnames){
require(plyr)
summary_func <- function(x, col){
c(mean = mean(x[[col]], na.rm=TRUE),
sd = sd(x[[col]], na.rm=TRUE))
}
data_sum<-ddply(data, groupnames, .fun=summary_func,
varname)
data_sum <- rename(data_sum, c("mean" = varname))
return(data_sum)
}
add_violin <- function(data,name,col) {
return()
}
add_errorbar <- function(name,data) {
s <- data_summary(data,name,c())
return()
}
data$sum = rowSums(data[,c('save','dispatch','load','exec')])
stat = data_summary(data,'sum',c())
p <- p + geom_violin(data=data,aes(x = !!file, y = sum)) +
stat_summary(fun.y=mean,geom="point",size=2,shape=24) +
geom_point(aes(x=!!file,y= !!stat$sum)) +
geom_label_repel(aes(x=!!file,y= !!stat$sum,
label=round(!!stat$sum,0),
),
nudge_y = -000,
nudge_x = 0.2
) +
geom_errorbar(aes(ymin = !! stat$sum - !! stat$sd,
ymax = !! stat$sum + !! stat$sd,
x = !!file,
width = 0.2
))
}
p <- p + xlab("Configuration")  + ylab("Total duration [ms]") +
scale_y_continuous(breaks=seq(0,20000,1000))
ggplot_build(p)
